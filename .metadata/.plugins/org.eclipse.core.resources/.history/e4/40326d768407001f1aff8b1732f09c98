package com.kh.test.main;

import java.util.Scanner;
import java.util.regex.Pattern;

public class 촋쐆킨怠 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.print("홈╂ 첎췒핖 : ");
		String 첎 = sc.nextLine();
		
		//찣쟍 왤쮩 좧줯핎 퀧콾첇 쌰-R 홈╂퍘晝
		if (Pattern.matches("^[쌰-R]*$", 첎)) {
			System.out.println("첎췒 졽첔찘챸 홈╂얦얯.");
		} else {
			System.out.println("홈┯ 첎췒촸쨮.");
		}
			//=> 쵔쟞 뱌칩입 젏챻 1썁쟞 첎췒핒 "홈┯ 첎췒촸쨮" 狗췃
		
		//첲턄좾탺 0 - 9 첎췒핎 쫔캽 쵖改줮쐑
		System.out.print("첲턄좾탺쟍 -첇 쮲첇 첎췒핖. : ");
		String 첲턄좾탺 = sc.nextLine();
		
		if (Pattern.matches("^[0-9]*$", 첲턄좾탺)) {
			System.out.println("첎췒 졽첔찘챸 첲턄좾탺 쫛첎얦얯.");
			//Integer.parseInt(첲턄좾탺)
		} else {
			System.out.println("첲턄좾탺 쫛첇 쮅얫얦얯.");
		}
		
	}
}
