package ncs.test5;

public class BookArrayTest {
	public static void main(String[] args) {
		Book[] bArray = new Book[3];
		
		bArray[0] = new Book("자바의 정석", "남궁성", 30000, "도우출판", 0.1);
		bArray[1] = new Book("자바의 정석", "남궁성", 30000, "도우출판", 0.1);
		bArray[2] = new Book("자바의 정석", "남궁성", 30000, "도우출판", 0.1);
		bArray[3] = new Book("자바의 정석", "남궁성", 30000, "도우출판", 0.1);
		
		//for-each문을 사용해서 출력
		
	}
}
