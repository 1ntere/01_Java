package ncs4.test2;

public class Fruit implements Comparable<Fruit> {
//필드
	String name;
	int preice;
	int quantity;
//메서드
	//기본생성자 ctrl + space
	public Fruit() {
		// TODO Auto-generated constructor stub
	}
	//필수생성자 과일명 과일가격 수량 alt+shift+s -> o
	public Fruit(String name, int preice, int quantity) {
		this.name = name;
		this.preice = preice;
		this.quantity = quantity;
	}
	//@Override toString alt+shift+s -> s
	@Override
	public String toString() {
		return "name=" + name
				+ ", preice=" + preice
				+ ", quantity=" + quantity;
	}
	//@Override
	@Override
	public int compareTo(Friut 과일비교) {
		return this.name.compareTo(name);
	}
	
}
