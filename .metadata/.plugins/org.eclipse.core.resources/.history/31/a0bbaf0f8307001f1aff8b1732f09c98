package com.kh.ArrayEx;
/*
이중배열
	배열과 마찬가지로 행과 열로 구성
	각 행은 서로 다른 배열을 나타냄
	각 열은 해당 해으이 요소(데이터)들을 나타냄
	
	자료형[][] 변수명 = new 자료형[행의크기][열의크기];
	자료형 변수명[][] = new 자료형[행의크기][열의크기];
	
	초기값을 처음부터 갖고 있는 이중 배열을 선언
	자료형 변수명[][] = {  {   }, {   }  };
	
	예시
	int arr[][] = new[2][3];
	
	arr은	행이	0
				1
	 	 	열이 0 1 2인 배열을 갖게 됨

*/
public class 이중배열 {

}
