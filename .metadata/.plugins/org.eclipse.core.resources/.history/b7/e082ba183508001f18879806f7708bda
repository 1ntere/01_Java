package com.kh.goodShop;

import java.util.*;

public class 컴퓨터가게 {
	public static void main(String[] args) {
		Map<String, Integer> 모든제품 = new HashMap<>();
		
		모든제품.put("노트북", 700000);
		모든제품.put("데스크탑", 20000);
		모든제품.put("모니터", 5000);
		모든제품.put("마우스", 3000);
		모든제품.put("키보드", 2000);
		
		//for-each문 - Map.Entry<>와 모든제품.entrySet()
		System.out.println("=== 컴퓨터 상품 목록 ===");
		for (Map.Entry<String, Integer> e : 모든제품.entrySet()) {
			String 제품 = e.getKey();
			int 가격 = e.getValue();
			System.out.println(제품 + "의 가격은 " + 가격 + "원 입니다.");
		}
	}
}
